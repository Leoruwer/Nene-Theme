{
	"name": "Ruwer's Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#1b1b1b",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#416279",
		"sideBarTitle.foreground": "#000000"
	},
	"tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#556B2F"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.single.php",
        "string.quoted.double.php",
        "string.quoted.single.html",
        "string.quoted.double.html",
        "string.quoted.single.js",
        "string.quoted.double.js"
      ],
      "settings": {
        "foreground": "#54a0a5"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#c581ffff"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#c581ffff"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#c581ffff"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffa400ff"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff0068ff"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#ff0068ff"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#94a4ff"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#bfff00ff"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#bfff00ff"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#bcdb5d"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa200ff"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#ff0068ff"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#bfff00ff"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#4dffffff"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#4dffffff"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4dffffff"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ffff19ff"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#ff0068ff",
        "fontStyle": "",
        "foreground": "#ffffffff"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#c581ffff",
        "foreground": "#ffffffff"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "meta.function-call",
        "support.function.name",
        "meta.function-call punctuation.definition.parameters",
        "meta.function-call variable.function",
        "meta.function-call entity.name.function.js",
        "support.function.source.rust",
        "meta.function-call meta.function-call.generic",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#ffeb3b",
        "fontStyle": " italic"
      }
    },
    {
      "name": "tag PHP",
      "scope": "punctuation.section.embedded",
      "settings": {
        "fontStyle": " bold",
        "foreground": "#6573C3"
      }
    }
	]
}
